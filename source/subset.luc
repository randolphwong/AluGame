module subset (
    input set[64],
    input index[6],
    output out[8]
  ) {

  always {
    out[0] = set[index];
    out[1] = set[index + 1];
    out[2] = set[index + 2];
    out[3] = set[index + 3];
    out[4] = set[index + 4];
    out[5] = set[index + 5];
    out[6] = set[index + 6];
    out[7] = set[index + 7];
  }
}
